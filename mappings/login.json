{
  "request": {
      "method": "POST",
      "url": "/login",
      "bodyPatterns": [
        {
          "matchesJsonPath": {
            "expression": "$.username",
            "equalTo": "mariomendonca",
            "caseInsensitive": true
          }
        }
      ]
  },
  "response": {
      "status": 200,
      "bodyFileName": "loginResponse.json",
      "headers": {
        "Content-Type": "application/json"
      },
      "fixedDelayMilliseconds": 3000
  }
}
